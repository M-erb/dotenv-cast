{
  "name": "dotenv-cast",
  "version": "1.0.1",
  "description": "Dead simple functional dotenv varaible type caster. No dependencies. Designed to be used with dotenv.",
  "author": "merb",
  "main": "dist/umd/dotenvCast.js",
  "exports": {
    "import": "./dist/mjs/dotenvCast.mjs",
    "require": "./dist/umd/dotenvCast.cjs"
  },
  "files": [
    "dist/umd",
    "dist/mjs",
    "dist/typings/"
  ],
  "license": "MIT",
  "scripts": {
    "start": "npm run build",
    "build": "npm run test && rollup -c",
    "test": "jest",
    "prepublish": "npm run test && rollup -c --silent"
  },
  "keywords": [
    "dotenv",
    "dot env",
    "cast",
    "casting",
    "env",
    ".env",
    "environment",
    "variables"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/M-erb/dotenv-cast.git"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/jest": "^29.4.0",
    "babel-jest": "^29.4.3",
    "eslint": "^8.35.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.4.3",
    "rollup": "^2.79.1",
    "rollup-plugin-copy": "^3.4.0"
  }
}
